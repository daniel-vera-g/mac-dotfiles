# Neovim
alias v="nvim"

# Git
alias g="git"
alias gs="git status"
alias gam="git add . && git commit -m"
alias gc="git checkout"
alias gcb="git checkout -b"

# General
alias x+="chmod +x"

# cli apps
# Show hidden and details
alias n="nnn -dH"
alias lzg="lazygit"

# Docker
alias dk="docker"
alias dkps="docker ps"
alias dkpsa="docker ps -a"

# Compose
alias c="docker-compose"
